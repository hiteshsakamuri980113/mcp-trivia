# MCP Trivia Backend Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Generative AI Configuration
# Set to TRUE to use Vertex AI, FALSE to use standard Google AI API
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Your Google AI API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# CORS Origins - allowed frontend URLs
# Add your frontend URLs here (comma-separated in JSON array format)
CORS_ORIGINS=["http://localhost:5173", "http://localhost:3000"]

# Optional: Add other environment variables as needed
# PORT=8000
# DEBUG=TRUE
